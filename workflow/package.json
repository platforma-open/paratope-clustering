{
  "name": "@platforma-open/milaboratories.paratope-clustering.workflow",
  "version": "1.0.2",
  "type": "module",
  "description": "Tengo-based template",
  "scripts": {
    "build": "shx rm -rf dist && pl-tengo check && pl-tengo build",
    "format": "/usr/bin/env emacs --script ./format.el",
    "do-pack": "shx rm -f *.tgz && pnpm pack && shx mv *.tgz package.tgz"
  },
  "dependencies": {
    "@platforma-open/milaboratories.paratope-clustering.software": "workspace:*",
    "@platforma-open/soedinglab.software-mmseqs2": "catalog:",
    "@platforma-sdk/workflow-tengo": "catalog:"
  },
  "devDependencies": {
    "@platforma-sdk/tengo-builder": "catalog:"
  }
}
